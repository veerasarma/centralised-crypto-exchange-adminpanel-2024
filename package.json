  "scripts": {
    "prebuild": "rm -rf ../build/demo-admin-panel/build",
    "build": "REACT_APP_MODE=production GENERATE_SOURCEMAP=false react-scripts build",
    "postbuild": "mv build ../build/production-admin-panel/",
    "prebuild:demo": "rm -rf ../build/demo-control/build",
